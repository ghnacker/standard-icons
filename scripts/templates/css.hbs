/**
 * {{title}}{{#if version}}, {{version}}{{/if}}
{{#if author}} * Created by {{author}}{{/if}}
{{#if homepage}} * {{homepage}}{{/if}}
{{#if license}} * {{license}} License{{/if}}
 */
@charset "UTF-8";

@font-face {
  font-family: "{{name}}";
{{#if fonts.eot}}
  src: url('{{name}}.eot');
{{/if}}
  src: {{#if fonts.eot}}url('{{name}}.eot?#iefix') format('eot'),{{/if}}
{{#if fonts.woff2}}    url('{{name}}.woff2') format('woff2'),{{/if}}
{{#if fonts.woff}}    url('{{name}}.woff') format('woff'),{{/if}}
{{#if fonts.ttf}}    url('{{name}}.ttf') format('truetype'),{{/if}}
{{#if fonts.svg}}    url('{{name}}.svg#{{name}}') format('svg'){{/if}};
}

.{{class}} {
  font-family: "{{name}}";
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  font-style: normal;
  font-variant: normal;
  font-weight: normal;
  speak: none;
  text-decoration: none;
  text-transform: none;
}


{{#each icons}}
.{{../prefix}}{{name}}:before {
  content: "{{{content}}}";
}
{{/each}}

