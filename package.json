    "tools/*"
  ],
  "scripts": {
    "start": "fractal start --sync",
    "bootstrap": "lerna bootstrap --hoist",
    "clean": "lerna clean",
    "distclean": "lerna clean --yes && rm -rf node_modules",
    "diff": "lerna diff",
    "updated": "lerna updated || exit 0",
    "lint": "lerna run lint",
    "release": "scripts/global/release",
    "publish-skipgit": "lerna publish --skip-git",
    "bump": "lerna publish --exact --skip-npm --since \"v$(npm info icon version)\"",
    "new-module": "scripts/global/new-module"
  },
